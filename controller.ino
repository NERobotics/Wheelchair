void setup() {
  // put your setup code here, to run once:
  pinMode(A0, INPUT); // x position of joystick
  pinMode(A1, INPUT); // y position of joystick
  pinMode(8, INPUT); // Button for emergency breaks
  pinMode(7, INPUT); // Button for Lights
  pinMode(3, INPUT); // Button for Speed Control (Turbo Mode)
  pinMode(9, INPUT); // Button for Power Locking
}

